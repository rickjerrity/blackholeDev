{
  "name": "blackhole-dev",
  "private": true,
  "description": "blackhole.dev Gatsby site",
  "version": "1.0.0",
  "author": "rickjerrity",
  "scripts": {
    "build": "gatsby clean && cross-env NODE_OPTIONS='--openssl-legacy-provider' gatsby build",
    "dev": "npm run develop",
    "develop": "gatsby clean && cross-env NODE_OPTIONS='--openssl-legacy-provider' gatsby develop",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,jsx,json}\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-format lint build",
    "start": "npm run develop",
    "serve": "cross-env NODE_OPTIONS='--openssl-legacy-provider' gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "gatsby": "^2.27.0",
    "gatsby-image": "^2.6.0",
    "gatsby-plugin-anchor-links": "^1.2.0",
    "gatsby-plugin-manifest": "^2.7.0",
    "gatsby-plugin-offline": "^3.5.0",
    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-sharp": "^2.9.0",
    "gatsby-remark-autolink-headers": "^2.11.0",
    "gatsby-remark-copy-linked-files": "^2.5.0",
    "gatsby-remark-images": "^3.6.0",
    "gatsby-source-filesystem": "^2.6.0",
    "gatsby-transformer-remark": "^2.11.0",
    "gatsby-transformer-sharp": "^2.7.0",
    "lodash.kebabcase": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0"
  }
}
